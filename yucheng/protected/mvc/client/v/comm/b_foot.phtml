<script type="text/javascript">
	function cw1(i,x,y)
	{
		var w=i.offsetWidth;
		var h=w*y/x;
		i.style.height=h+"px";
	}
	function gbc(p,x,y) {
		var tags = tags || document.getElementsByTagName("div");
		var list = [];
		for( var k in tags)
		{
			var tag = tags[k];
			if(tag.className == p) {
				cw1(tag,x,y);
				list.push(tag);
			}
		}
		return list;
	}
</script>

